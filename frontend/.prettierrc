{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": false,
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 100,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "@/components|@/pages|@/partials|@/routing",
    "@/pages/omni",
    "@/hooks|@/store|@/api",
    "@/utils|@/config|@/types",
    "@/styles",
    "^..?/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
